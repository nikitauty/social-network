{
  "name": "social-network",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint -c linters/.eslintrc.cjs src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "slint": "stylelint -c linters/.stylelintrc.cjs \"src/**/*.sass\"",
    "tlint": "tsc",
    "lint:fix": "yarn lint --fix",
    "slint:fix": "stylelint --fix -c linters/.stylelintrc.cjs \"src/**/*.sass\"",
    "lint:all": "yarn lint && yarn tlint && yarn slint ",
    "lint:all:fix": "yarn lint:fix && yarn slint:fix",
    "preview": "vite preview"
  },
  "prettier": "./linters/.prettierrc.cjs",
  "eslintConfig": {
    "extends": "./linters/.eslintrc.cjs"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.364.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.7",
    "vite-plugin-svgr": "^4.2.0"
